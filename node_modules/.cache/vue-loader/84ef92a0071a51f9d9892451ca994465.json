nding-sections\">\n    <div class=\"feature-section\">\n\n      <div v-if=\"selectedRecord.length > 0\" class=\"container\">\n        <div class=\"row py-5\">\n          <div class=\"col-12 text-left\">\n            <a class=\"apply-btn text-xs text-heavy pull-right\" href=\"#application\">Apply Now</a>\n            <div class=\"text-md text-heavy\">{{selectedRecord[0].title}} - {{selectedRecord[0].location}}</div>\n            <div class=\"d-inline text-sm\">at {{selectedRecord[0].company}}</div>\n          </div>\n        </div>\n        <div class=\"row\">\n          <div class=\"col-12 py-5\">\n            <div class=\"text-sm text-heavy\">About this role</div>\n            <div class=\"text-sm\">{{selectedRecord[0].about}}</div>\n          </div>\n        </div>\n        <div class=\"row\">\n          <div class=\"col-12 py-5\">\n            <div class=\"text-sm text-heavy\">What you'll do</div>\n            <div class=\"text-sm\">{{selectedRecord[0].responsibilities}}</div>\n          </div>\n        </div>\n        <div class=\"row\">\n          <div class=\"col-12 py-5\">\n            <div class=\"text-sm text-heavy\">Who you are</div>\n            <div class=\"text-sm\">{{selectedRecord[0].requirements}}</div>\n          </div>\n        </div>\n        <div class=\"row pt-5\">\n          <div class=\"col-12\" id=\"application\">\n            <iframe class=\"airtable-embed airtable-dynamic-height\" :src=\"'https://airtable.com/embed/shrAlb0QRgnPPDke5?prefill_Linked+Job='+selectedRecord[0].title\" frameborder=\"0\" onmousewheel=\"\" width=\"100%\" height=\"2050\" style=\"background: transparent; border: 1px solid #ccc;\"></iframe>\n          </div>\n        </div>\n      </div>\n\n      <div v-else class=\"container\" id=\"about\">\n\n        <div class=\"row\">\n          <div class=\"col-12 pt-5\">\n            <div class=\"title-text\" @click=\"runPrint()\">\n              Current Job Openings\n            </div>\n          </div>\n        </div>\n        <div class=\"row align-items-center\">\n          <div class=\"col-6 text-left\">\n            <div class=\"searchbar-box\">\n              <font-awesome-icon class=\"searchbar-icon\" icon=\"search\" />\n              <input v-model=\"searchQuery\" class=\"searchbar-input\" type=\"text\" placeholder=\"Filter Jobs\">\n            </div>\n          </div>\n          <div class=\"col-6 text-right\">\n            <span v-if=\"searchQuery.length > 0\">Filtered results:</span>\n            <span v-else>Total jobs:</span>\n            {{openingCount}}\n          </div>\n        </div>\n\n        <div v-if=\"noResults\" class=\"row align-items-center\">\n          <div class=\"col-12 py-5\">\n            No results found.  Try searching with new keywords, or <span class=\"search-reset\" @click=\"searchQuery = ''\">browse all openings.</span>\n          </div>\n        </div>\n\n        <div class=\"row\">\n          <div class=\"col-12 py-5\">\n\n            <div v-for=\"item in filteredRecords\" v-bind:key=\"item.id\" class=\"record-items\">\n              <div class=\"row\">\n                <div class=\"col-4\">\n                  <h1 @click=\"selectRecord(item)\">{{item.fields.Title}}</h1>\n                </div>\n                <div class=\"col-4\">\n                  <a :href=\"item.fields.CompanyURL\" target=\"_blank\">{{item.fields.Company}}</a>\n                </div>\n                <div class=\"col-4\">\n                  <p>{{item.fields.Location}}</p>\n                </div>\n              </div>\n            </div>\n\n          </div>\n        </div>\n\n      </div>\n\n    </div>\n  </div>\n</div>\n</template>\n\n<script src=\"https://static.airtable.com/js/embed/embed_snippet_v1.js\"></script>\n<script>\nimport axios from 'axios'\nexport default {\n  name: 'careers',\n  computed: {\n    openingCount () {\n      return this.filteredRecords.length\n    },\n    selectedRecords () {\n      let x = this.selectedRecord\n      return this.records.filter(function (item) {\n        if (x.length > 0) {\n          if (item.id == x) {\n            return item\n          }\n        }\n      })\n    },\n    noResults() {\n      if (this.filteredRecords.length == 0) {\n        return true\n      } else {\n        return false\n      }\n    },\n    filteredRecords () {\n      let x = this.searchQuery\n      return this.records.filter(function (item) {\n        if (x.length > 0) {\n          if (item.fields.Company.toLowerCase().includes(x.toLowerCase()) || item.fields.Title.toLowerCase().includes(x.toLowerCase()) || item.fields.Location.toLowerCase().includes(x.toLowerCase())) {\n            return item\n          }\n        } else { return item }\n      })\n    }\n  },\n  data () {\n    return {\n      selectedRecord: [],\n      searchQuery: '',\n      apiUrl: 'https://api.airtable.com/v0/appTpsUnXDAGQRlRh/Openings?api_key=key8jxgDOppA4mRnp',\n      records: []\n    }\n  },\n  methods: {\n    selectRecord (item) {\n      console.log(item)\n      this.selectedRecord.push({\n        id: item.id,\n        title: item.fields.Title,\n        company: item.fields.Company,\n        companyURL: item.fields.CompanyURL,\n        location: item.fields.Location,\n        about: item.fields.About,\n        responsibilities: item.fields.Responsibilities,\n        requirements: item.fields.Requirements,\n      })\n      document.body.scrollTop = 0\n      document.documentElement.scrollTop = 0\n    },\n    runPrint () {\n        console.log(this.selectedRecord[0].title)\n    },\n  },\n  mounted () {\n    axios({\n      url: this.apiUrl,\n      params: {\n        view: 'Main View'\n      }\n    }).then((res) => {\n      this.records = res.data.records\n    })\n  }\n}\n</script>\n\n<style scoped>\n#landing-feature-sm {\n  background-color: #435465;\n  padding: 145px 0 22px;\n}\n#landing-feature {\n  background-color: #435465;\n  padding: 145px 0 100px;\n}\n#landing-sections {\n  font-family: 'Avenir';\n  background-color: #fff;\n  color: #313131;\n}\