}\n  },\n  data () {\n    return {\n      selectedRecord: [],\n      searchQuery: '',\n      apiUrl: 'https://api.airtable.com/v0/appM37BH6TQnpG3wL/Portfolio?api_key=key8jxgDOppA4mRnp',\n      records: []\n    }\n  },\n  methods: {\n    selectRecord (item) {\n      this.selectedRecord = []\n      console.log(item)\n      this.selectedRecord.push({\n        id: item.id,\n        company: item.fields.Company,\n        title: item.fields.Title,\n        description: item.fields.Description,\n        url: item.fields.CompanyURL\n      })\n    },\n    runPrint () {\n      console.log(this.records)\n    }\n  },\n  mounted () {\n    axios({\n      url: this.apiUrl\n    }).then((res) => {\n      this.records = res.data.records\n    })\n  }\n}\n",{"version":3,"sources":["portfolio.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAkFA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"portfolio.vue","sourceRoot":"src/components","sourcesContent":["<template>\n<div>\n  <div id=\"landing-feature-n\">\n    <div class=\"benno-lg-container\">\n      <div class=\"crazy-border\"></div>\n      <div class=\"row\">\n        <div class=\"col-12\">\n          <div class=\"text-lg text-heavy text-left\">\n            Innovators and new ventures\n          </div>\n        </div>\n        <div class=\"col-12 col-sm-10 col-xl-7 mr-auto\">\n          <div class=\"text-sm\">\n            Entrepreneurs and companies powered by Benno Technologies are original, innovative, and bold. They embrace the need to bring extraordinary\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n\n  <div id=\"landing-sections\">\n    <div class=\"feature-section\">\n      <div class=\"benno-lg-container\" id=\"about\">\n        <div class=\"row\">\n\n          <div class=\"col-12\">\n            <div class=\"featured-items pt-0\">\n              <div class=\"record-items\" v-for=\"item in featuredRecords\" v-bind:key=\"item.id\">\n                <div class=\"row\">\n                  <div class=\"col-12\">\n                    <div class=\"record-text-top-cat\">\n                      SPOTLIGHT\n                    </div>\n                  </div>\n                  <div class=\"col-12\">\n                    <div class=\"row\">\n                      <div class=\"col-12 py-lg-5\">\n                        <div class=\"text-md text-heavy text-left text-lg-center\">\n                          {{item.fields.Company}}!\n                        </div>\n                      </div>\n                      <div class=\"col-12\">\n                        <div class=\"text-sm\">\n                          {{item.fields.Description}}\n                        </div>\n                        <div class=\"row\">\n                          <div class=\"col-12 text-right\">\n                            <div class=\"text-sm icon-link\">\n                              <a :href=\"item.fields.CompanyURL\" target=\"_blank\">Visit Website</a>\n                            </div>\n                          </div>\n                        </div>\n                      </div>\n                    </div>\n                  </div>\n                </div>\n              </div>\n              <div class=\"text-items\">\n                <div class=\"\">\n                  <div class=\"row\">\n                    <div class=\"col-12\">\n\n                      <div class=\"row align-items-center py-5\">\n                        <div v-for=\"item in filteredRecords\" v-bind:key=\"item.id\" class=\"col-6 col-md-4 col-lg-3 portfolio-block\">\n                          <div @click=\"selectRecord(item)\" class=\"bb-item text-xs\">{{item.fields.Company}}</div>\n                        </div>\n                      </div>\n\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</div>\n</template>\n\n<script>\nimport axios from 'axios'\nexport default {\n  name: 'portfolio',\n  computed: {\n    selectedRecords () {\n      let x = this.selectedRecord\n      return this.records.filter(function (item) {\n        if (x.length > 0) {\n          if (item.id === x) {\n            return item\n          }\n        }\n      })\n    },\n    openingCount () {\n      return this.records.length\n    },\n    featuredRecords () {\n      return this.records.filter(function (item) {\n        if (item.fields.Featured) {\n          return item\n        }\n      })\n    },\n    filteredRecords () {\n      let x = this.searchQuery\n      return this.records.filter(function (item) {\n        if (x.length > 0) {\n          if (item.fields.Company.toLowerCase().includes(x.toLowerCase())) {\n            return item\n          }\n        } else { return item }\n      })\n    }\n  },\n  data () {\n    return {\n      selectedRecord: [],\n      searchQuery: '',\n      apiUrl: 'https://api.airtable.com/v0/appM37BH6TQnpG3wL/Portfolio?api_key=key8jxgDOppA4mRnp',\n      records: []\n    }\n  },\n  methods: {\n    selectRecord (item) {\n      this.selectedRecord = []\n      console.log(item)\n      this.selectedRecord.push({\n        id: item.id,\n        company: item.fields.Company,\n        title: item.fields.Title,\n        description: item.fields.Description,\n        url: item.fields.CompanyURL\n      })\n    },\n    runPrint () {\n      console.log(this.records)\n    }\n  },\n  mounted () {\n    axios({\n      url: this.apiUrl\n    }).then((res) => {\n      this.records = res.data.records\n    })\n  }\n}\n</script>\n\n<style scoped>\n\n.text-link a {\n  border-color: #0084e1 !important;\n}\n.text-link a:hover {\n  color: #0084e1 !important;\n}\n\n.text-link a:hover::after {\n  color: #0084e1 !important;\n}\n\n.inline-link a {\n  border-color: #0084e1 !important;\n}\n.inline-link a:hover {\n  color: #0084e1 !important;\n}\n.inline-link a:hover::after {\n  color: #0084e1 !important;\n}\n\n.record-text-top-cat::after {\n  background: repeating-linear-gradient(60deg, #0084e1, #0084e1 1px, #fff 1px, #fff 1.25px) !important;\n}\n\n@media (min-width: 992px) {\n  .featured-items {\n    width: calc(100% - 250px) !important;\n    margin-left: 250px !important;\n  }\n}\n@media (min-width: 1200px) {\n  .featured-items {\n    width: calc(100% - 500px) !important;\n    margin-left: 250px !important;\n  }\n}\n\n.featured-items::before {\n  background: repeating-linear-gradient(60deg, #0084e1, #0084e1 1px, #fff 1px, #fff 1.25px);\n  top: 0;\n}\n\n.crazy-border {\n  background: repeating-linear-gradient(60deg, #0084e1, #0084e1 1px, #fff 1px, #fff 1.25px) !important;\n}\n\n.portfolio-block {\n  text-align: left;\n}\n.portfolio-block .bb-item {\n  padding: 10px 0;\n  color: #000;\n  display: block;\n  border-top: 3px solid #000;\n  margin-bottom: 40px;\n}\n.portfolio-block .bb-item:hover {\n  cursor: pointer;\n  text-decoration: none;\n  color: #0084e1;\n  border-top: 3px solid #0084e1;\n}\n\n.portfolio-block .bb-item:hover::before {\n  content: '';\n  border-top: 2px solid #0084e1;\n  position: absolute;\n  top: 8px;\n  left: 15px;\n  width: calc(100% - 30px);\n}\n\n.record-items:hover .icon-link::after {\n  transform: rotate(360deg) !important;\n  transition: 1s;\n  color: #0084e1;\n}\n\n.icon-link a {\n  color: #000;\n}\n.record-items:hover .icon-link a {\n  color: #0084e1 !important;\n  text-decoration: none !important;\n}\n\n</style>\n"]}]}