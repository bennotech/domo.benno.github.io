ink a:hover {\n  color: #f9676b !important;\n}\n\n.text-link a:hover::after {\n  color: #f9676b !important;\n}\n\n.inline-link a {\n  border-color: #f9676b !important;\n}\n.inline-link a:hover {\n  color: #f9676b !important;\n}\n.inline-link a:hover::after {\n  color: #f9676b !important;\n}\n\n.record-items {\n  margin-bottom: 100px !important;\n}\n.record-text-top-cat::after {\n  background: repeating-linear-gradient(60deg, #f9676b, #f9676b 1px, #fff 1px, #fff 1.25px) !important;\n}\n\n@media (min-width: 992px) {\n  .featured-items {\n    width: calc(100% - 250px) !important;\n    margin-left: 250px !important;\n  }\n}\n\n.featured-items::before {\n  background: repeating-linear-gradient(60deg, #f9676b, #f9676b 1px, #fff 1px, #fff 1.25px);\n  top: 0;\n}\n\n.crazy-border {\n  background: repeating-linear-gradient(60deg, #f9676b, #f9676b 1px, #fff 1px, #fff 1.25px) !important;\n}\n\n</style>\n"]}]}